{
  "name": "@garage-catalyst/ibm-garage-cloud-cli",
  "version": "0.0.72",
  "description": "CLI that wraps IBM Cloud CLI to simplify build and deploy image functions",
  "main": "dist/index.js",
  "author": "Sean Sundberg <seansund@us.ibm.com>",
  "license": "MIT",
  "private": false,
  "bin": {
    "igc": "dist/script.js"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/logs/",
      "/pacts/",
      "/coverage/"
    ],
    "coverageDirectory": "coverage",
    "setupFiles": [
      "jest-plugin-context/setup"
    ]
  },
  "scripts": {
    "pretest": "npm run build",
    "test": "jest",
    "tdd": "jest --watch",
    "build": "tsc -d",
    "postbuild": "chmod +x dist/script.js",
    "prelink": "npm run build",
    "link": "npm link $npm_package_name",
    "unlink": "npm unlink $npm_package_name",
    "prepare": "npm run build",
    "igc": "ts-node src/script.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/dockerode": "^2.5.19",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.2",
    "@types/open": "^6.1.0",
    "@types/ora": "^3.2.0",
    "@types/puppeteer": "^1.12.4",
    "@types/puppeteer-core": "^1.9.0",
    "@types/rewire": "^2.5.28",
    "@types/superagent": "^4.1.1",
    "@types/yaml": "^1.0.2",
    "@types/yargs": "^13.0.0",
    "babel-jest": "^24.7.1",
    "jest": "^24.7.1",
    "jest-plugin-context": "^2.9.0",
    "rewire": "^4.0.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.3"
  },
  "dependencies": {
    "dockerode": "^2.5.8",
    "open": "^6.3.0",
    "ora": "^3.4.0",
    "puppeteer": "^1.18.0",
    "superagent": "^5.1.0",
    "tslib": "^1.10.0",
    "yaml": "^1.6.0",
    "yargs": "^13.2.2"
  }
}
